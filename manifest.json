{
  "manifest_version": 2,

  "name": "Cookie Jar",
  "description": "This Extension will read and detail cookies",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://www.readers.com/",
    "http://www.sunglasswarehouse.com/",
    "http://www.felixandiris.com/",
    "http://*/",
    "https://*/"
  ]
}